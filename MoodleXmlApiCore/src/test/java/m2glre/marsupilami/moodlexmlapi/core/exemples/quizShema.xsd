<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="quiz" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="name">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="text" type="xs:string" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="answer">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="text" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
        <xs:element name="tolerance" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
        <xs:element name="tolerancetype" type="xs:string" minOccurs="0" msdata:Ordinal="3" />
        <xs:element name="correctanswerformat" type="xs:string" minOccurs="0" msdata:Ordinal="4" />
        <xs:element name="correctanswerlength" type="xs:string" minOccurs="0" msdata:Ordinal="6" />
        <xs:element name="feedback" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="text" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="fraction" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="quiz" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="name" />
        <xs:element ref="answer" />
        <xs:element name="question">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="image" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
              <xs:element name="image_base64" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
              <xs:element name="defaultgrade" type="xs:string" minOccurs="0" msdata:Ordinal="3" />
              <xs:element name="penalty" type="xs:string" minOccurs="0" msdata:Ordinal="4" />
              <xs:element name="hidden" type="xs:string" minOccurs="0" msdata:Ordinal="5" />
              <xs:element name="single" type="xs:string" minOccurs="0" msdata:Ordinal="6" />
              <xs:element name="answernumbering" type="xs:string" minOccurs="0" msdata:Ordinal="7" />
              <xs:element name="usecase" type="xs:string" minOccurs="0" msdata:Ordinal="8" />
              <xs:element ref="name" minOccurs="0" maxOccurs="unbounded" />
              <xs:element name="questiontext" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                  </xs:sequence>
                  <xs:attribute name="format" type="xs:string" />
                </xs:complexType>
              </xs:element>
              <xs:element name="generalfeedback" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="shuffleanswers" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="shuffleanswers_Text" msdata:Ordinal="0">
                    <xs:extension base="xs:string">
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="subquestion" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                    <xs:element ref="answer" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element ref="answer" minOccurs="0" maxOccurs="unbounded" />
              <xs:element name="units" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="unit" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="multiplier" type="xs:string" minOccurs="0" />
                          <xs:element name="unit_name" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="dataset_definitions" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="dataset_definition" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="type" type="xs:string" minOccurs="0" />
                          <xs:element name="itemcount" type="xs:string" minOccurs="0" />
                          <xs:element name="number_of_items" type="xs:string" minOccurs="0" />
                          <xs:element name="status" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="text" type="xs:string" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element ref="name" minOccurs="0" maxOccurs="unbounded" />
                          <xs:element name="distribution" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="text" type="xs:string" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="minimum" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="text" type="xs:string" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="maximum" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="text" type="xs:string" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="decimals" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="text" type="xs:string" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="dataset_items" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="dataset_item" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="number" type="xs:string" minOccurs="0" />
                                      <xs:element name="value" type="xs:string" minOccurs="0" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="category" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="correctfeedback" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="partiallycorrectfeedback" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="incorrectfeedback" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="text" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="type" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>