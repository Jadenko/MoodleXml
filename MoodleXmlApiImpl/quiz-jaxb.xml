        <hidden>false</hidden>
        <imageUrl>backupdata/446px-Uncle_Sam_pointing_finger_.jpg</imageUrl>
        <imageBase64>/9j/4AAQSkZJRgABAQEAqwCrAAD/2wBDAAYEBQYFBAY</imageBase64>
        <bsingle>true</bsingle>
        <bshuffleanswers>true</bshuffleanswers>
        <lanswer ffraction="-100.0">
