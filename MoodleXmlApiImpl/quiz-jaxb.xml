    <question xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="multipleChoiceQuestion" type="multichoice">
        <name>Architectures N tiers</name>
        <questiontext oformat="moodle_auto_format">
            <text>Cocher les assertions vraies.</text>
        </questiontext>
        <generalFeedBack>Rappel N tiers : architectures multi-niveaux pouvant être distribuées sur plusieurs noeuds physique d'un réseau.</generalFeedBack>
        <penalty>0.1</penalty>
        <defaultGrade>1.0</defaultGrade>
        <hidden>false</hidden>
        <imageBase64> /9j/4AAQSkZJRgABAQEAqwCrAAD/2wBDAAYEBQYFBAYGBQYHBwYIChAKCgkJChQOD</imageBase64>
        <bsingle>false</bsingle>
        <bshuffleanswers>true</bshuffleanswers>
        <lanswer ffraction="0.0">
            <stext>Une architecture N-tiers est uniquement une architecture à base de Web Services</stext>
            <sfeedback>Une architecture distribuée peut reposer par exemple sur RMI</sfeedback>
        </lanswer>
        <lanswer ffraction="33.0">
            <stext>Une architecture client serveur est une architecture N-tiers</stext>
            <sfeedback>Nada.</sfeedback>
        </lanswer>
        <lanswer ffraction="33.0">
            <stext>Une architecture N-tiers correspond à une architecture d'application distribuée sur plusieurs noeuds physiques</stext>
            <sfeedback>Nada.</sfeedback>
        </lanswer>
        <lanswer ffraction="33.0">
            <stext>Une application web est une application reposant sur une architecture N Tiers</stext>
            <sfeedback>Nada.</sfeedback>
        </lanswer>
